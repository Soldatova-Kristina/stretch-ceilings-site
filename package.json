{
  "name": "stretch-ceilings-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next .swc node_modules/.cache && next dev",
    "build": "next build",
    "build:timeweb": "cp .env.timeweb .env.production && next build",
    "build:timeweb-subfolder": "cp .env.timeweb-subfolder .env.production && next build",
    "build:gh": "cp .env.gh-pages .env.production && next build",
    "deploy:gh": "npm run build:gh && touch out/.nojekyll && rm -rf /tmp/gh-pages-deploy && cp -R out /tmp/gh-pages-deploy && npx gh-pages --dist /tmp/gh-pages-deploy --dotfiles --repo https://github.com/Soldatova-Kristina/stretch-ceilings-site.git",
    "postbuild": "node scripts/fix-css-paths.mjs",
    "start": "npx serve out",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "15.5.5",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "gh-pages": "^6.3.0",
    "serve": "^14.2.3",
    "sharp": "^0.33.0"
  }
}
